datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Deposit {
  amount    Float?
  country   String?
  createdAt DateTime @default(now())
  currency  String?
  id        String   @id @default(cuid())
  updatedAt DateTime @updatedAt
}
